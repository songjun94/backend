<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Insert title here</title>
</head>
<body>
	<%
		String name2 =request.getParameter("name2");
	%>
	// ?name2=jerry&name=tom 으로 작성하면  아래의 주석처럼 웹화면에 뜬다
	
	Hello, <%= name2  %><br/> <!--  Hello, Jerry -->
	Hello, ${ param.name }<br/> <!--  Hello, Tom -->
	
	
</body>
</html>