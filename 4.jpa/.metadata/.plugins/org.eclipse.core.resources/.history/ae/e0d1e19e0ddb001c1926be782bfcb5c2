public class App {

	public static void main(String[] args) {
		
		//JPA의 인터페이스 활용
		
		// JDBC 인터페이스
		EntityManagerFactory emf = Persistence.createEntityManagerFactory("step01");
		
		Book book = new Book();
	}

}
